{
  "name": "pipeline-project",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "prepare": "husky",

    "lint": "eslint .",

    "format:check": "prettier . --check",
    "format:write": "prettier . --write",

    "test": "jest --coverage --coverageReporters=lcov"
  },
  "dependencies": {
    "cookie-parser": "~1.4.7",
    "debug": "~4.4.0",
    "express": "~4.21.2",
    "morgan": "~1.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/jest": "^30.0.0",
    "eslint": "^10.0.0",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "supertest": "^7.2.2"
  },
  "lint-staged": {
    "**/*.{js,ts,mjs,cjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{html,css,md,y?ml}": [
      "prettier --write"
    ]
  }
}